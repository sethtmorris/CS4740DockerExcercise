{
   "hosting":{
      "public":"tutorial/public",
      "cleanUrls": true,
      "trailingSlash": false,
      "headers":[
         {
            "source":"**/*.@(eot|otf|ttf|ttc|woff|font.css)",
            "headers":[
               {
                  "key":"Access-Control-Allow-Origin",
                  "value":"*"
               }
            ]
         },
         {
            "source":"**/*.@(jpg|jpeg|gif|png)",
            "headers":[
               {
                  "key":"Cache-Control",
                  "value":"max-age=86400"
               }
            ]
         }
      ]
   }
}
